>\n\n  <script>\n    (function() {\n      'use strict';\n      \n      \/\/ Slideshow functionaliteit\n      let currentSlide = 0;\n      const totalSlides = 7;\n      const slides = document.querySelectorAll('.mlb-slideshow-slide');\n      const dots = document.querySelectorAll('.mlb-slideshow-dot');\n      \n      \/\/ Check of elementen bestaan\n      if (!slides.length || !dots.length) return;\n      \n      \/\/ Functie om naar specifieke slide te gaan\n      function goToSlide(index) {\n        \/\/ Verwijder active class van alle slides en dots\n        slides.forEach(slide => slide.classList.remove('active'));\n        dots.forEach(dot => dot.classList.remove('active'));\n        \n        \/\/ Voeg active class toe aan huidige slide en dot\n        slides[index].classList.add('active');\n        dots[index].classList.add('active');\n        \n        currentSlide = index;\n      }\n      \n      \/\/ Functie om naar volgende slide te gaan\n      function nextSlide() {\n        let next = (currentSlide + 1) % totalSlides;\n        goToSlide(next);\n      }\n      \n      \/\/ Click handler voor dots\n      dots.forEach((dot, index) => {\n        dot.addEventListener('click', function() {\n          goToSlide(index);\n        });\n      });\n      \n      \/\/ Auto-advance elke 4 seconden\n      setInterval(nextSlide, 4000);\n      \n      \/\/ InitiÃ«le setup\n      goToSlide(0);\n    })();\n  <\/script>\n<\/div>","color_scheme":"","padding_top":28,"padding_bottom":0,"ignore_spacing":false}}},"order":["cart-items","cart-footer","quick_info_bar","image_gallery_9Ua4QD","custom_liquid_3MNXbF"]}